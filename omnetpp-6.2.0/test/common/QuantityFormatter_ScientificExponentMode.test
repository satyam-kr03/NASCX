%description:
Test quantity formatter scientific exponent mode.

%includes:

#include "../lib/quantityformattertester.h"

using namespace omnetpp::common;

%activity:

QuantityFormatter quantityFormatter;

QuantityFormatter::Options options1;
options1.notationMode = QuantityFormatter::NotationMode::SCIENTIFIC;
options1.scientificExponentMode = QuantityFormatter::ScientificExponentMode::RANGE_1_10;
QuantityFormatter quantityFormatter1(options1);

testQuantityFormatter(quantityFormatter1, 1, -9, 9, nullptr, {digits123etc});

QuantityFormatter::Options options2;
options2.notationMode = QuantityFormatter::NotationMode::SCIENTIFIC;
options2.scientificExponentMode = QuantityFormatter::ScientificExponentMode::RANGE_01_1;
QuantityFormatter quantityFormatter2(options2);

testQuantityFormatter(quantityFormatter2, 1, -9, 9, nullptr, {digits123etc});

QuantityFormatter::Options options3;
options3.notationMode = QuantityFormatter::NotationMode::SCIENTIFIC;
options3.scientificExponentMode = QuantityFormatter::ScientificExponentMode::MOD_3;
QuantityFormatter quantityFormatter3(options3);

testQuantityFormatter(quantityFormatter3, 1, -9, 9, nullptr, {digits123etc});

